{
  "name": "@berkide/js-client",
  "version": "0.1.0",
  "description": "JavaScript/TypeScript client for BerkIDE headless editor server",
  "author": "Berk Co≈üar <lookmainpoint@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/berkide/berkide-js-client.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc --project tsconfig.cjs.json",
    "clean": "rm -rf dist",
"prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "berkide",
    "editor",
    "client",
    "headless",
    "ide"
  ]
}
